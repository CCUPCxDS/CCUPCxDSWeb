{
  "name": "vite-js-tailwindcss-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "npx tailwindcss -i ./src/css/index.css -o ./src/css/tailwind.css --minify && vite",
    "build": "vite build",
    "preview": "vite preview",
    "css": "npx tailwindcss -i ./src/css/index.css -o ./src/css/tailwind.css --minify --watch",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "vue": "^3.2.25",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.7",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.1",
    "vite": "^2.9.9"
  }
}
